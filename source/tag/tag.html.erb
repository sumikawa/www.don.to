layout: site
title: dummy title as the below line overwrite this line
---
<% current_page.data.title = "Tag: #{tag_name}" %>

<%
    index = []
    tags_list[tag_name].each do |file_path|
      header = YAML.load_file(file_path)
      if header['draft'].nil?
        index << gen_link(file_path, header['title'], false)
      end
    end
%>
<dl>
<%= index.sort.reverse.join("\n") %>
</dl>
